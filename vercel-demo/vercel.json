{
  "version": 2,
  "name": "meteorological-dashboard",
  "builds": [
    {
      "src": "api/weather.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/weather",
      "dest": "/api/weather.js"
    }
  ],
  "regions": ["hkg1"],
  "functions": {
    "api/weather.js": {
      "maxDuration": 10
    }
  }
}